# Environment variables for Parkinson's Multiagent System
# Copy this file to .env and configure with your actual values

# Groq API Configuration
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL_CHAT=llama3-8b-8192
GROQ_MODEL_ANALYSIS=llama3-70b-8192
GROQ_MODEL_REPORT=llama3-70b-8192
GROQ_MAX_TOKENS=4096
GROQ_TEMPERATURE=0.1
GROQ_TIMEOUT=30

# Database Configuration
DATABASE_URL=sqlite:///./data/parkinsons_system.db
DATABASE_ECHO=false
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=20

# System Configuration
LOG_LEVEL=INFO
DEBUG_MODE=false

# Optional: Production database (PostgreSQL example)
# DATABASE_URL=postgresql://user:password@localhost:5432/parkinsons_db

# Optional: Redis for enhanced caching
# REDIS_URL=redis://localhost:6379/0